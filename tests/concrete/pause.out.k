<conf>
  <esdt>
    <is-running>
      false
    </is-running>
    <meta>
      <meta-steps>
        .
      </meta-steps>
      <meta-incoming>
        1 M|-> .TxList
      </meta-incoming>
      <meta-out-txs>
        .TxList
      </meta-out-txs>
      <global-token-settings>
        <global-token-setting>
          <global-token-id>
            1
          </global-token-id>
          <global-token-paused>
            true
          </global-token-paused>
          <global-token-owner>
            accountAddr ( 1 , "A1" )
          </global-token-owner>
          <global-token-props>
            canAddSpecialRoles P|-> true
            canChangeOwner P|-> false
            canFreeze P|-> false
            canPause P|-> true
            canUpgrade P|-> true
            canWipe P|-> false
          </global-token-props>
        </global-token-setting>
      </global-token-settings>
    </meta>
    <shards>
      <shard>
        <shard-id>
          1
        </shard-id>
        <incoming-txs>
          #metachainShardId M|-> .TxList
        </incoming-txs>
        <user-txs>
          .TxList
        </user-txs>
        <steps>
          .
        </steps>
        <current-tx>
          #nullTx
        </current-tx>
        <out-txs>
          .TxList
        </out-txs>
        <accounts>
          <account>
            <account-name>
              "A1"
            </account-name>
            <is-sc>
              false
            </is-sc>
            <esdt-balances>
              .BalMap
            </esdt-balances>
          </account>
        </accounts>
        <snapshot>
          #emptySnapshot
        </snapshot>
        <token-settings>
          <token-setting>
            <token-setting-id>
              1
            </token-setting-id>
            <limited>
              false
            </limited>
            <paused>
              true
            </paused>
            <frozen>
              .Set
            </frozen>
          </token-setting>
        </token-settings>
        <logs>
          .Logs
        </logs>
      </shard> <shard>
        <shard-id>
          2
        </shard-id>
        <incoming-txs>
          #metachainShardId M|-> .TxList
        </incoming-txs>
        <user-txs>
          .TxList
        </user-txs>
        <steps>
          .
        </steps>
        <current-tx>
          #nullTx
        </current-tx>
        <out-txs>
          .TxList
        </out-txs>
        <accounts>
          .AccountCellMap
        </accounts>
        <snapshot>
          #emptySnapshot
        </snapshot>
        <token-settings>
          <token-setting>
            <token-setting-id>
              1
            </token-setting-id>
            <limited>
              false
            </limited>
            <paused>
              true
            </paused>
            <frozen>
              .Set
            </frozen>
          </token-setting>
        </token-settings>
        <logs>
          .Logs
        </logs>
      </shard>
    </shards>
  </esdt>
</conf>
