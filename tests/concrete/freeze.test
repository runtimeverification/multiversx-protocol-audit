addShard(1)
addShard(2)
addAccount(1, "A1", false)
addAccount(2, "A2", false)

addUserTx(1, 
    issue( accountAddr(1, "A1"), 1, 100 ) { canFreeze: true }
)
#wait

addUserTx(1, 
    freeze(accountAddr(1 , "A1" ), accountAddr(2 , "A2" ), 1, true)
)

#wait

assert("A2 is frozen", 
       SetItem("A2") == getFrozen(2, 1))
assert("Empty Frozen in Shard 1", 
       .Set == getFrozen(1, 1))
